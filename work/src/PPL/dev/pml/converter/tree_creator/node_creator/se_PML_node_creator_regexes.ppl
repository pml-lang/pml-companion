service PML_node_creator_regexes

    attributes type:regex
        paragraph_break = regex.create ( '''\r?\n[ \t]*\r?\n''' )
        whitespace = regex.create ( '''[ \t\r\n]+''' )

        comma_separator = regex.create ( '''\s*,\s*''' )
        tab_separator   = regex.create ( '''\s*\t\s*''' )
    .
.

factory PML_header_node_HTML_writer type:PML_node_HTML_writer<PML_header_node>

    function write

        context.write_block_node_with_child_nodes (
            i_node,
            HTML_tag_name = "div"
            CSS_class_name = "pml-header"
            append_new_line_for_tag = no )
    .

    creator create kind:in_all
.
